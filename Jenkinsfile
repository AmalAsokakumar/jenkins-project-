pipeline{
    agent any
    stages{
        stage('build'){
            where {
                
            }
            steps{
                echo 'this is a build stage'
            }
        }
        stage('test'){
            steps{
                echo 'testing the artifact'
                
        }
    }
        stage('deploy'){
            steps{ 
                echo "testing completed successfully"
            }
    }
    }

}
